    "6c8d4248-75d3-4c51-a588-963d184d7cc9": {
      "type": "image_text",
      "blocks": {
        "template--19376943759681__6c8d4248-75d3-4c51-a588-963d184d7cc9-16858873043ab58179-1": {
          "type": "bl_text",
          "settings": {
            "col_dk": "6",
